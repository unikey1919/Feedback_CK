            app:destination="@id/nav_add_assignment" />
        <action
            android:id="@+id/action_nav_home_to_nav_do_fb"
            app:destination="@id/nav_do_fb" />


    </fragment>

    <fragment
        android:id="@+id/nav_assignment"
