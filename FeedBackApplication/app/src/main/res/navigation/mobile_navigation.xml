    </fragment>

    <fragment
        android:id="@+id/nav_edit_class"
        android:name="com.example.feedbackapplication.ui.classjoin.EditClassFragment"
        tools:layout="@layout/edit_class">
        <action
            android:id="@+id/action_nav_edit_class_to_nav_class"
